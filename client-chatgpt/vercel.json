{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*).css",
        "dest": "/$1.css"
      },
      {
        "src": "/(.*).woff2",
        "dest": "/$1.woff2"
      },
      {
        "src": "/(.*).png",
        "dest": "/$1.png"
      },
      {
        "src": "/(.*).svg",
        "dest": "/$1.svg"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
        }
        ]
        }  